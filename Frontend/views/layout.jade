doctype html
html(lang="en", ng-app="AirlineReservation")
  head
    title= title
    meta(charset="utf-8")
    meta(name='viewport', content='width=device-width, initial-scale=1.0')

    // FONT CSS
    link(type="text/css", rel='stylesheet', href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900")
    link(type="text/css", rel='stylesheet', href="https://fonts.googleapis.com/css?family=Montserrat:400,700")
    link(type="text/css", rel='stylesheet', href="/font/font-icon/font-awesome/css/font-awesome.min.css")
    link(type="text/css", rel='stylesheet', href="/font/font-flaticon/flaticon.css")

    // LIBRARY CSS
    link(type="text/css", rel="stylesheet", href="/libraries/animate.css/animate.css")
