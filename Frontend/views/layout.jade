  body
    // WRAPPER
    #wrapper-content
      // MAIN CONTENT
      .main-content
        section.page-banner.homepage-default
          .container
            .homepage-banner-warpper
              .homepage-banner-content
                .group-title
                  h1.title tvt airlines
                  p.text
                    | The world you have never seen
                    span.boder
                .group-btn
                  a.btn-click(href='#', data-hover='CLICK ME')
                    span.text go explore now
                    span.icons.fa.fa-long-arrow-right
        block content
    // FOOTER
    footer
      .footer-main.padding-top.padding-bottom
        .container
          .footer-main-wrapper
            a.logo-footer(href='index.html')
              img.img-responsive(src='/images/logo/logo.png', alt='')
            .row
              .col-2
                .col-md-3.col-xs-5
                  .contact-us-widget.widget
                    .title-widget contact us
                    .content-widget
                      .info-list
                        ul.list-unstyled
                          li
                            i.icons.fa.fa-map-marker
                            a.link(href='#') 132, My Street, Kingston, New York 12401
                          li
                            i.icons.fa.fa-phone
                            a.link(href='#') 270 - 188 - 6026
                          li
                            i.icons.fa.fa-envelope-o
                            a.link(href='#') domain@expooler.com
                      .form-email
                        p.text Sign up for our mailing list to get latest updates and offers.
                        form(action='index.html')
                          .input-group
                            input.form-control.form-email-widget(type='text', placeholder='Email address')
                            span.input-group-btn
                              button.btn-email(type='submit') ✔
                .col-md-2.col-xs-3
                  .booking-widget.widget.text-center
                    .title-widget book now
                    .content-widget
                      ul.list-unstyled
                        li
                          a.link(href='#') Flights
                        li
                          a.link(href='#') Tours
                        li
                          a.link(href='#') Packages
                        li
                          a.link(href='#') Transfer
                        li
                          a.link(href='#') Car Rent
                        li
                          a.link(href='#') Cruises
                .col-md-2.col-xs-4
                  .explore-widget.widget
                    .title-widget explore
                    .content-widget
                      ul.list-unstyled
                        li
                          a.link(href='#') Tour Singapore City
                        li
                          a.link(href='#') Tour Manila City
                        li
                          a.link(href='#') Tour New York City
                        li
                          a.link(href='#') Tour Sanghai City
                        li
                          a.link(href='#') Tour Hongkong City
                        li
                          a.link(href='#') Tour Tokyo City
              .col-2
                .col-md-2.col-xs-6
                  .top-deals-widget.widget
                    .title-widget top deals
                    .content-widget
                      ul.list-unstyled
                        li
                          a.link(href='#') Tour Packages Singapore
                        li
                          a.link(href='#') Tour Packages Manila
                        li
                          a.link(href='#') Tour Packages New York
                        li
                          a.link(href='#') Tour Packages Sanghai
                        li
